package com.sinaleju.lifecircle.app.model;

import java.io.Serializable;

import android.graphics.drawable.BitmapDrawable;

public class SendMsgImageModel implements Serializable {

	/**
	 * 
	 */
	private static final long serialVersionUID = 2381267021525108856L;

	private String imagePath;
	private BitmapDrawable drawable;

	public String getImagePath() {
		return imagePath;
	}

	public void setImagePath(String imagePath) {
		this.imagePath = imagePath;
	}

	public BitmapDrawable getDrawable() {
		return drawable;
	}

	public void setDrawable(BitmapDrawable drawable) {
		this.drawable = drawable;
	}

}
