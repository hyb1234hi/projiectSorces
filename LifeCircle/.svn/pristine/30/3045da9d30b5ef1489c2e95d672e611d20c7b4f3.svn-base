<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#EEEBE5"
    android:orientation="vertical" >

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="fill_parent"
        android:padding="10dp"
        android:scrollbars="none" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <LinearLayout
                android:layout_width="0px"
                android:layout_height="0px"
                android:focusable="true"
                android:focusableInTouchMode="true" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/login_background_grey"
                android:orientation="vertical"
                android:padding="2dp" >

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:paddingBottom="5dp"
                    android:paddingLeft="8dp"
                    android:paddingRight="8dp"
                    android:paddingTop="5dp" >

                    <TextView
                        style="@style/business_apply_auth_textview_style"
                        android:layout_width="90dp"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="昵称" />

                    <EditText
                        android:id="@+id/bus_name_edit"
                        style="@style/business_apply_auth_textview_style"
                        android:layout_width="fill_parent"
                        android:layout_height="match_parent"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="90dp"
                        android:layout_marginRight="40dp"
                        android:background="@null"
                        android:hint="商家名称"
                        android:maxLength="10"
                        android:singleLine="true" />

                    <Button
                        android:id="@+id/bus_name_edit_icon"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:background="@drawable/comm_edit_icon"
                        android:gravity="center"
                        android:paddingLeft="10dp"
                        android:paddingRight="10dp" />
                </RelativeLayout>

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/common_dividerline"
                    android:contentDescription="@string/imageDescription" />

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:paddingBottom="5dp"
                    android:paddingLeft="8dp"
                    android:paddingRight="8dp"
                    android:paddingTop="5dp" >

                    <TextView
                        style="@style/business_apply_auth_textview_style"
                        android:layout_width="90dp"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="登陆账号" />

                    <TextView
                        android:id="@+id/bus_login_number"
                        style="@style/business_apply_auth_textview_style"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="90dp" />
                </RelativeLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:background="@drawable/login_background_grey"
                android:orientation="vertical"
                android:padding="2dp" >

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:paddingBottom="5dp"
                    android:paddingLeft="8dp"
                    android:paddingRight="8dp"
                    android:paddingTop="5dp" >

                    <TextView
                        style="@style/business_apply_auth_textview_style"
                        android:layout_width="90dp"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="账号类别" />

                    <TextView
                        android:id="@+id/bus_login_type"
                        style="@style/business_apply_auth_textview_style"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="90dp"
                        android:text="商家" />
                </RelativeLayout>

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/common_dividerline"
                    android:contentDescription="@string/imageDescription" />

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:paddingBottom="5dp"
                    android:paddingLeft="8dp"
                    android:paddingRight="8dp"
                    android:paddingTop="5dp" >

                    <TextView
                        style="@style/business_apply_auth_textview_style"
                        android:layout_width="90dp"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="认证状态" />

                    <TextView
                        android:id="@+id/identity_type"
                        style="@style/business_apply_auth_textview_style"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="90dp"
                        android:singleLine="true"
                        android:text="未认证" />

                    <Button
                        android:id="@+id/identity_type_btn"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:background="@drawable/bus_approve_bg"
                        android:gravity="center"
                        android:padding="5dp"
                        android:text="申请V认证"
                        android:textColor="#666666"
                        android:textSize="16sp" />
                </RelativeLayout>

                <ImageView
                    android:id="@+id/dividerline_one"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/common_dividerline"
                    android:contentDescription="@string/imageDescription" />

                <RelativeLayout
                    android:id="@+id/industry_type_layout"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:paddingBottom="5dp"
                    android:paddingLeft="8dp"
                    android:paddingRight="8dp"
                    android:paddingTop="5dp" >

                    <TextView
                        style="@style/business_apply_auth_textview_style"
                        android:layout_width="90dp"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="服务行业" />

                    <TextView
                        android:id="@+id/industry_type"
                        style="@style/business_apply_auth_textview_style"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="90dp"
                        android:singleLine="true"
                        android:text="美食-快餐" />

                    <Button
                        android:id="@+id/industry_type_icon"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:background="@drawable/comm_edit_icon"
                        android:gravity="center"
                        android:paddingLeft="10dp"
                        android:paddingRight="10dp" />
                </RelativeLayout>

                <ImageView
                    android:id="@+id/dividerline_two"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/common_dividerline"
                    android:contentDescription="@string/imageDescription" />

                <RelativeLayout
                    android:id="@+id/style_type_layout"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:paddingBottom="5dp"
                    android:paddingLeft="8dp"
                    android:paddingRight="8dp"
                    android:paddingTop="5dp" >

                    <TextView
                        style="@style/business_apply_auth_textview_style"
                        android:layout_width="90dp"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="经营方式" />

                    <TextView
                        android:id="@+id/style_type"
                        style="@style/business_apply_auth_textview_style"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="90dp"
                        android:singleLine="true"
                        android:text="自营" />

                    <Button
                        android:id="@+id/style_type_icon"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:background="@drawable/comm_edit_icon"
                        android:gravity="center"
                        android:paddingLeft="10dp"
                        android:paddingRight="10dp" />
                </RelativeLayout>
            </LinearLayout>

            <TextView
                android:id="@+id/bus_type_text"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="2dp"
                android:layout_marginTop="15dp"
                android:text="商家信息"
                android:textColor="#666666"
                android:textSize="18sp" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/login_background_grey"
                android:orientation="vertical"
                android:padding="2dp" >

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:paddingBottom="5dp"
                    android:paddingLeft="8dp"
                    android:paddingRight="8dp"
                    android:paddingTop="5dp" >

                    <TextView
                        style="@style/business_apply_auth_textview_style"
                        android:layout_width="90dp"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="服务电话" />

                    <EditText
                        android:id="@+id/servic_phone_edit"
                        style="@style/business_apply_auth_textview_style"
                        android:layout_width="fill_parent"
                        android:layout_height="match_parent"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="90dp"
                        android:layout_marginRight="40dp"
                        android:background="@null"
                        android:hint=""
                        android:inputType="phone"
                        android:singleLine="true" />

                    <Button
                        android:id="@+id/servic_phone_icon"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:background="@drawable/comm_edit_icon"
                        android:gravity="center"
                        android:paddingLeft="10dp"
                        android:paddingRight="10dp" />
                </RelativeLayout>

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/common_dividerline"
                    android:contentDescription="@string/imageDescription" />

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:minHeight="50dp"
                    android:paddingBottom="5dp"
                    android:paddingLeft="8dp"
                    android:paddingRight="8dp"
                    android:paddingTop="5dp" >

                    <TextView
                        android:id="@+id/bus_address_name"
                        style="@style/business_apply_auth_textview_style"
                        android:layout_width="90dp"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="商家地址" />

                    <EditText
                        android:id="@+id/bus_address_edit"
                        style="@style/business_apply_auth_textview_style"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="90dp"
                        android:layout_marginRight="30dp"
                        android:background="@null"
                        android:hint=""
                        android:maxLength="30"
                        android:singleLine="false" />

                    <Button
                        android:id="@+id/bus_address_icon"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:background="@drawable/comm_edit_icon"
                        android:gravity="center"
                        android:paddingLeft="10dp"
                        android:paddingRight="10dp" />
                </RelativeLayout>

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/common_dividerline"
                    android:contentDescription="@string/imageDescription" />

                <RelativeLayout
                    android:id="@+id/map_address_layout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:minHeight="50dp"
                    android:paddingBottom="5dp"
                    android:paddingLeft="8dp"
                    android:paddingRight="8dp"
                    android:paddingTop="5dp" >

                    <TextView
                        style="@style/business_apply_auth_textview_style"
                        android:layout_width="90dp"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="地图标注" />

                    <TextView
                        android:id="@+id/map_address_edit"
                        style="@style/business_apply_auth_textview_style"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="90dp"
                        android:layout_marginRight="30dp"
                        android:background="@null"
                        android:maxLines="3"
                        android:singleLine="false" />

                    <Button
                        android:id="@+id/map_address_icon"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:background="@drawable/right_icon"
                        android:gravity="center" />
                </RelativeLayout>

                <ImageView
                    android:id="@+id/dividerline_three"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/common_dividerline"
                    android:contentDescription="@string/imageDescription" />

                <RelativeLayout
                    android:id="@+id/traffic_address_layout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:minHeight="50dp"
                    android:paddingBottom="5dp"
                    android:paddingLeft="8dp"
                    android:paddingRight="8dp"
                    android:paddingTop="5dp" >

                    <TextView
                        style="@style/business_apply_auth_textview_style"
                        android:layout_width="90dp"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="公交信息" />

                    <EditText
                        android:id="@+id/traffic_address_edit"
                        style="@style/business_apply_auth_textview_style"
                        android:layout_width="fill_parent"
                        android:layout_height="match_parent"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="90dp"
                        android:layout_marginRight="30dp"
                        android:background="@null"
                        android:hint=""
                        android:singleLine="false" />

                    <Button
                        android:id="@+id/traffic_address_icon"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:background="@drawable/comm_edit_icon"
                        android:gravity="center"
                        android:paddingLeft="10dp"
                        android:paddingRight="10dp" />
                </RelativeLayout>

                <ImageView
                    android:id="@+id/dividerline_four"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/common_dividerline"
                    android:contentDescription="@string/imageDescription" />

                <RelativeLayout
                    android:id="@+id/per_capita_layout"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:paddingBottom="5dp"
                    android:paddingLeft="8dp"
                    android:paddingRight="8dp"
                    android:paddingTop="5dp" >

                    <TextView
                        style="@style/business_apply_auth_textview_style"
                        android:layout_width="90dp"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="人均消费" />

                    <TextView
                        style="@style/business_apply_auth_textview_style"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="90dp"
                        android:text="￥" />

                    <EditText
                        android:id="@+id/per_capita_edit"
                        style="@style/business_apply_auth_textview_style"
                        android:layout_width="fill_parent"
                        android:layout_height="match_parent"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="110dp"
                        android:layout_marginRight="40dp"
                        android:background="@null"
                        android:maxLength="10"
                        android:singleLine="true" />

                    <Button
                        android:id="@+id/per_capita_icon"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:background="@drawable/comm_edit_icon"
                        android:gravity="center"
                        android:paddingLeft="10dp"
                        android:paddingRight="10dp" />
                </RelativeLayout>

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/common_dividerline"
                    android:contentDescription="@string/imageDescription" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="150dp"
                    android:orientation="horizontal"
                    android:paddingLeft="8dp"
                    android:paddingRight="8dp" >

                    <TextView
                        android:id="@+id/hours_time"
                        style="@style/business_apply_auth_textview_style"
                        android:layout_width="50dp"
                        android:layout_height="fill_parent"
                        android:layout_gravity="center_vertical"
                        android:gravity="center_vertical"
                        android:text="营业\n时间" />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="fill_parent"
                        android:background="@drawable/split_line_shu"
                        android:contentDescription="@string/imageDescription" />

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:orientation="vertical" >

                        <LinearLayout
                            android:layout_width="fill_parent"
                            android:layout_height="0dp"
                            android:layout_weight="1"
                            android:orientation="horizontal" >

                            <TextView
                                style="@style/business_apply_auth_textview_style"
                                android:layout_width="60dp"
                                android:layout_height="fill_parent"
                                android:layout_gravity="center_vertical"
                                android:gravity="center"
                                android:text="周一到\n周五" />

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="fill_parent"
                                android:orientation="vertical" >

                                <RelativeLayout
                                    android:id="@+id/top_morning_layout"
                                    android:layout_width="fill_parent"
                                    android:layout_height="0dp"
                                    android:layout_weight="1"
                                    android:gravity="center_vertical"
                                    android:orientation="horizontal" >

                                    <TextView
                                        style="@style/business_apply_auth_textview_style"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_centerVertical="true"
                                        android:text="上午" />

                                    <TextView
                                        android:id="@+id/top_morning_edit"
                                        style="@style/business_apply_auth_textview_style"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_centerVertical="true"
                                        android:layout_marginLeft="40dp"
                                        android:text="08:00-11:00" />

                                    <Button
                                        android:id="@+id/top_morning_icon"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_alignParentRight="true"
                                        android:layout_centerVertical="true"
                                        android:background="@drawable/comm_edit_icon"
                                        android:gravity="center"
                                        android:paddingLeft="10dp"
                                        android:paddingRight="10dp" />
                                </RelativeLayout>

                                <RelativeLayout
                                    android:id="@+id/top_afternoon_layout"
                                    android:layout_width="fill_parent"
                                    android:layout_height="0dp"
                                    android:layout_weight="1"
                                    android:gravity="center_vertical"
                                    android:orientation="horizontal" >

                                    <TextView
                                        style="@style/business_apply_auth_textview_style"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_centerVertical="true"
                                        android:text="下午" />

                                    <TextView
                                        android:id="@+id/top_afternoon_edit"
                                        style="@style/business_apply_auth_textview_style"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_centerVertical="true"
                                        android:layout_marginLeft="40dp"
                                        android:text="13:00-19:00" />

                                    <Button
                                        android:id="@+id/top_afternoon_icon"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_alignParentRight="true"
                                        android:layout_centerVertical="true"
                                        android:background="@drawable/comm_edit_icon"
                                        android:gravity="center"
                                        android:paddingLeft="10dp"
                                        android:paddingRight="10dp" />
                                </RelativeLayout>
                            </LinearLayout>
                        </LinearLayout>

                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="@drawable/common_dividerline"
                            android:contentDescription="@string/imageDescription" />

                        <LinearLayout
                            android:layout_width="fill_parent"
                            android:layout_height="0dp"
                            android:layout_weight="1"
                            android:orientation="horizontal" >

                            <TextView
                                style="@style/business_apply_auth_textview_style"
                                android:layout_width="60dp"
                                android:layout_height="fill_parent"
                                android:layout_gravity="center_vertical"
                                android:gravity="center"
                                android:text="周六\n周日" />

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="fill_parent"
                                android:orientation="vertical" >

                                <RelativeLayout
                                    android:id="@+id/bottom_morning_layout"
                                    android:layout_width="fill_parent"
                                    android:layout_height="0dp"
                                    android:layout_weight="1"
                                    android:gravity="center_vertical"
                                    android:orientation="horizontal" >

                                    <TextView
                                        style="@style/business_apply_auth_textview_style"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_centerVertical="true"
                                        android:text="上午" />

                                    <TextView
                                        android:id="@+id/bottom_morning_edit"
                                        style="@style/business_apply_auth_textview_style"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_centerVertical="true"
                                        android:layout_marginLeft="40dp"
                                        android:text="09:00-11:00" />

                                    <Button
                                        android:id="@+id/bottom_morning_icon"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_alignParentRight="true"
                                        android:layout_centerVertical="true"
                                        android:background="@drawable/comm_edit_icon"
                                        android:gravity="center"
                                        android:paddingLeft="10dp"
                                        android:paddingRight="10dp" />
                                </RelativeLayout>

                                <RelativeLayout
                                    android:id="@+id/bottom_afternoon_layout"
                                    android:layout_width="fill_parent"
                                    android:layout_height="0dp"
                                    android:layout_weight="1"
                                    android:gravity="center_vertical"
                                    android:orientation="horizontal" >

                                    <TextView
                                        style="@style/business_apply_auth_textview_style"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_centerVertical="true"
                                        android:text="下午" />

                                    <TextView
                                        android:id="@+id/bottom_afternoon_edit"
                                        style="@style/business_apply_auth_textview_style"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_centerVertical="true"
                                        android:layout_marginLeft="40dp"
                                        android:text="14:00-19:00" />

                                    <Button
                                        android:id="@+id/bottom_afternoon_icon"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_alignParentRight="true"
                                        android:layout_centerVertical="true"
                                        android:background="@drawable/comm_edit_icon"
                                        android:gravity="center"
                                        android:paddingLeft="10dp"
                                        android:paddingRight="10dp" />
                                </RelativeLayout>
                            </LinearLayout>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/common_dividerline"
                    android:contentDescription="@string/imageDescription" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:paddingBottom="5dp"
                    android:paddingLeft="8dp"
                    android:paddingRight="8dp"
                    android:paddingTop="5dp" >

                    <TextView
                        android:id="@+id/bus_introduce"
                        style="@style/business_apply_auth_textview_style"
                        android:layout_width="90dp"
                        android:layout_height="wrap_content"
                        android:text="商家介绍" />

                    <EditText
                        android:id="@+id/bus_introduce_edit"
                        android:layout_width="fill_parent"
                        android:layout_height="match_parent"
                        android:background="@null"
                        android:hint=""
                        android:maxLines="4"
                        android:textColor="@color/dark_black"
                        android:textSize="14sp" />

                    <RelativeLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content" >

                        <Button
                            android:id="@+id/bus_introduce_icon"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:background="@drawable/comm_edit_icon"
                            android:paddingLeft="10dp"
                            android:paddingRight="10dp" />
                    </RelativeLayout>
                </LinearLayout>

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/common_dividerline"
                    android:contentDescription="@string/imageDescription" />

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:paddingBottom="10dp"
                    android:paddingLeft="8dp"
                    android:paddingRight="8dp"
                    android:paddingTop="10dp" >

                    <TextView
                        android:id="@+id/bus_image"
                        style="@style/business_apply_auth_textview_style"
                        android:layout_width="90dp"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="商家图片" />

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="80dp"
                        android:orientation="vertical" >

                        <LinearLayout
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="10dp"
                            android:orientation="horizontal" >

                            <ImageView
                                android:id="@+id/top_one_image"
                                android:layout_width="60dp"
                                android:layout_height="40dp"
                                android:layout_marginRight="5dp"
                                android:padding="2dp"
                                android:scaleType="fitXY"
                                android:src="@drawable/bus_infor_add_image" />

                            <ImageView
                                android:id="@+id/top_two_image"
                                android:layout_width="60dp"
                                android:layout_height="40dp"
                                android:layout_marginRight="5dp"
                                android:scaleType="fitXY"
                                android:src="@drawable/bus_infor_add_image" />

                            <ImageView
                                android:id="@+id/top_three_image"
                                android:layout_width="60dp"
                                android:layout_height="40dp"
                                android:layout_marginRight="5dp"
                                android:scaleType="fitXY"
                                android:src="@drawable/bus_infor_add_image" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal" >

                            <ImageView
                                android:id="@+id/bottom_one_image"
                                android:layout_width="60dp"
                                android:layout_height="40dp"
                                android:layout_marginRight="5dp"
                                android:scaleType="fitXY"
                                android:src="@drawable/bus_infor_add_image" />

                            <ImageView
                                android:id="@+id/bottom_two_image"
                                android:layout_width="60dp"
                                android:layout_height="40dp"
                                android:layout_marginRight="5dp"
                                android:scaleType="fitXY"
                                android:src="@drawable/bus_infor_add_image" />

                            <ImageView
                                android:id="@+id/bottom_three_image"
                                android:layout_width="60dp"
                                android:layout_height="40dp"
                                android:layout_marginRight="5dp"
                                android:scaleType="fitXY"
                                android:src="@drawable/bus_infor_add_image" />
                        </LinearLayout>
                    </LinearLayout>
                </RelativeLayout>
            </LinearLayout>

            <TextView
                android:id="@+id/spe_title"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="2dp"
                android:layout_marginTop="15dp"
                android:text="商家特惠信息"
                android:textColor="#666666"
                android:textSize="18sp" />

            <LinearLayout
                android:id="@+id/spe_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/login_background_grey"
                android:orientation="vertical"
                android:padding="2dp" >

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:minHeight="50dp"
                    android:paddingBottom="5dp"
                    android:paddingLeft="8dp"
                    android:paddingRight="8dp"
                    android:paddingTop="5dp" >

                    <TextView
                        style="@style/business_apply_auth_textview_style"
                        android:layout_width="90dp"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="标题" />

                    <EditText
                        android:id="@+id/title_edit"
                        style="@style/business_apply_auth_textview_style"
                        android:layout_width="fill_parent"
                        android:layout_height="match_parent"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="90dp"
                        android:layout_marginRight="30dp"
                        android:background="@null"
                        android:hint=""
                        android:maxLength="16"
                        android:singleLine="false" />

                    <Button
                        android:id="@+id/title_edit_icon"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:background="@drawable/comm_edit_icon"
                        android:gravity="center"
                        android:paddingLeft="10dp"
                        android:paddingRight="10dp" />
                </RelativeLayout>

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/common_dividerline"
                    android:contentDescription="@string/imageDescription" />

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:paddingBottom="5dp"
                    android:paddingLeft="8dp"
                    android:paddingRight="8dp"
                    android:paddingTop="5dp" >

                    <TextView
                        style="@style/business_apply_auth_textview_style"
                        android:layout_width="90dp"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="原价" />

                    <TextView
                        style="@style/business_apply_auth_textview_style"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="90dp"
                        android:hint="￥" />

                    <EditText
                        android:id="@+id/list_price_edit"
                        style="@style/business_apply_auth_textview_style"
                        android:layout_width="fill_parent"
                        android:layout_height="match_parent"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="110dp"
                        android:layout_marginRight="40dp"
                        android:background="@null"
                        android:hint="500"
                        android:maxLength="11"
                        android:singleLine="true" />

                    <Button
                        android:id="@+id/list_price_icon"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:background="@drawable/comm_edit_icon"
                        android:gravity="center"
                        android:paddingLeft="10dp"
                        android:paddingRight="10dp" />
                </RelativeLayout>

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/common_dividerline"
                    android:contentDescription="@string/imageDescription" />

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:paddingBottom="5dp"
                    android:paddingLeft="8dp"
                    android:paddingRight="8dp"
                    android:paddingTop="5dp" >

                    <TextView
                        style="@style/business_apply_auth_textview_style"
                        android:layout_width="90dp"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="优惠价格" />

                    <TextView
                        style="@style/business_apply_auth_textview_style"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="90dp"
                        android:text="￥" />

                    <EditText
                        android:id="@+id/pricing_edit"
                        android:layout_width="fill_parent"
                        android:layout_height="match_parent"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="110dp"
                        android:layout_marginRight="40dp"
                        android:background="@null"
                        android:maxLength="11"
                        android:singleLine="true"
                        android:textColor="#E31E1E"
                        android:textSize="16sp" />

                    <Button
                        android:id="@+id/pricing_icon"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:background="@drawable/comm_edit_icon"
                        android:gravity="center"
                        android:paddingLeft="10dp"
                        android:paddingRight="10dp" />
                </RelativeLayout>

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/common_dividerline"
                    android:contentDescription="@string/imageDescription" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:paddingBottom="5dp"
                    android:paddingLeft="8dp"
                    android:paddingRight="8dp"
                    android:paddingTop="5dp" >

                    <TextView
                        style="@style/business_apply_auth_textview_style"
                        android:layout_width="90dp"
                        android:layout_height="wrap_content"
                        android:text="特惠说明" />

                    <EditText
                        android:id="@+id/specials_edit"
                        android:layout_width="fill_parent"
                        android:layout_height="match_parent"
                        android:background="@null"
                        android:hint=""
                        android:maxLines="4"
                        android:textColor="@color/dark_black"
                        android:textSize="14sp" />

                    <RelativeLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content" >

                        <Button
                            android:id="@+id/specials_icon"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:background="@drawable/comm_edit_icon"
                            android:paddingLeft="10dp"
                            android:paddingRight="10dp" />
                    </RelativeLayout>
                </LinearLayout>

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/common_dividerline"
                    android:contentDescription="@string/imageDescription" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:paddingBottom="5dp"
                    android:paddingLeft="8dp"
                    android:paddingRight="8dp"
                    android:paddingTop="5dp" >

                    <TextView
                        style="@style/business_apply_auth_textview_style"
                        android:layout_width="90dp"
                        android:layout_height="wrap_content"
                        android:text="特惠图片" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="3dp"
                        android:text="横版宣传图(560X140)"
                        android:textColor="@color/dark_black"
                        android:textSize="14sp" />

                    <ImageView
                        android:id="@+id/specials_image"
                        android:layout_width="fill_parent"
                        android:layout_height="100dp"
                        android:layout_marginRight="5dp"
                        android:scaleType="fitXY"
                        android:src="@drawable/big_add_image_btn" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="5dp"
                        android:layout_marginTop="5dp"
                        android:text="宣传小图(尽量保证图片4:3比例)"
                        android:textColor="@color/dark_black"
                        android:textSize="14sp" />

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="20dp"
                        android:orientation="horizontal" >

                        <FrameLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="20dp"
                            android:layout_weight="1" >

                            <ImageView
                                android:id="@+id/specials_left_image"
                                android:layout_width="fill_parent"
                                android:layout_height="60dp"
                                android:scaleType="fitXY"
                                android:src="@drawable/bus_infor_add_image" />
                        </FrameLayout>

                        <FrameLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="20dp"
                            android:layout_weight="1" >

                            <ImageView
                                android:id="@+id/specials_mid_image"
                                android:layout_width="fill_parent"
                                android:layout_height="60dp"
                                android:scaleType="fitXY"
                                android:src="@drawable/bus_infor_add_image" />
                        </FrameLayout>

                        <FrameLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1" >

                            <ImageView
                                android:id="@+id/specials_right_image"
                                android:layout_width="fill_parent"
                                android:layout_height="60dp"
                                android:scaleType="fitXY"
                                android:src="@drawable/bus_infor_add_image" />
                        </FrameLayout>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </ScrollView>

</LinearLayout>