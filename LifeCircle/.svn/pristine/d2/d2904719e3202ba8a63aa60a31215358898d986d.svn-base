<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="style_picker" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">false</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:windowAnimationStyle">@+android:style/Animation.InputMethod</item>
    </style>

    <style name="dialog_normal" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">false</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:windowAnimationStyle">@+android:style/Animation.Dialog</item>
    </style>

    <style name="style_dialog_visiting_card" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowAnimationStyle">@style/style_window_anim_visiting_card</item>
    </style>
    <style name ="style_window_anim_visiting_card">
        <item name="android:windowEnterAnimation">@anim/anim_visiting_card_drop_in</item>
        <item name="android:windowExitAnimation">@anim/anim_visiting_card_drop_out</item>
    </style>
    <style name="style_dialog_visiting_card_low_api" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowAnimationStyle">@style/style_window_anim_visiting_card_low_api</item>
    </style>
    <style name ="style_window_anim_visiting_card_low_api">
        <item name="android:windowEnterAnimation">@anim/anim_visiting_card_drop_in_for_low_api</item>
        <item name="android:windowExitAnimation">@anim/anim_visiting_card_drop_out</item>
    </style>
    <style name="style_dialog_search_top_bar" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">false</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowAnimationStyle">@style/style_window_anim_search_top_bar</item>
    </style>
    <style name ="style_window_anim_search_top_bar">
        <item name="android:windowEnterAnimation">@anim/anim_search_top_bar_drop_in</item>
        <item name="android:windowExitAnimation">@anim/anim_search_top_bar_raise_out</item>
    </style>

</resources>